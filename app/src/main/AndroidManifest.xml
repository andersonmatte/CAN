<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="br.com.can">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/can"
        android:label="@string/app_name"
        android:roundIcon="@drawable/can"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".activity.CadastroJogoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.JogosActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.PerfilLogadoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ContatoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.HistoriaActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.CadastroActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.LoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.JogadoresActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.JogoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ClubeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.HomeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.SplashScreenActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>